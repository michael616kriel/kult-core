{
  "branches": "master",
  "repositoryUrl": "https://github.com/michael616kriel/kult-core",
  "debug": "false",
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "docs",
            "scope": "README",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          }
        ],
        "parserOpts": {
          "noteKeywords": [
            "BREAKING CHANGE",
            "BREAKING CHANGES"
          ]
        }
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": true,
        "tarballDir": "build"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": [
          "build/**"
        ]
      }
    ]
  ]
}
